class Solution {
    public boolean hasGroupsSizeX(int[] deck) {
        Map<Integer,Integer> map=new HashMap<>();
        for(int d:deck) map.put(d,map.getOrDefault(d,0)+1);
        int g=0;
        for(int v:map.values()) g=gcd(g,v);
        return g>=2;
    }
    int gcd(int a,int b){ return b==0?a:gcd(b,a%b); }
}
