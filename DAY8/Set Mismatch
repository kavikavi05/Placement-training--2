class Solution {
    public int[] findErrorNums(int[] nums) {
        boolean[] seen = new boolean[nums.length + 1];
        int dup = -1, miss = -1;

        for (int n : nums) {
            if (seen[n]) dup = n;
            seen[n] = true;
        }
        for (int i = 1; i <= nums.length; i++) {
            if (!seen[i]) miss = i;
        }
        return new int[]{dup, miss};
    }
}
